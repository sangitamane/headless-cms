{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "https://springernature.com/eds-c-footer.schema.json",
    "title": "Footer",
    "description": "Footer component",
    "type": "object",
    "properties": {
      "dataTest": { "type": "string" },
      "dataTrackComponent": { "type": "string" },
      "navigation": {
        "type": "object",
        "properties": {
          "links": {
            "type": "array",
            "items": {
              "type": "object",
              "properties": {
                "text": { "type": "string" },
                "buttonProperties": { "type": "string" },
                "url": { "type": "string" },
                "dataAttributes": {
                  "type": "array",
                  "items": {
                    "type": "object",
                    "properties": {
                      "name": {
                        "type": "string",
                        "enum": [
                          "test",
                          "track",
                          "track-context",
                          "track-value",
                          "track-method",
                          "track-content-type",
                          "track-item-id",
                          "track-type",
                          "track-action",
                          "track-category",
                          "track-label",
                          "track-component"
                        ]
                      },
                      "value": {
                        "type": "string"
                      }
                    },
                    "required": [
                      "name",
                      "value"
                    ]
                  }
                }
              },
              "required": ["text"]
            }
          }
        },
        "required": ["links"]
      },
      "productGroups": {
        "type": "array",
        "items": {
          "type": "object",
          "properties": {
            "title": { "type": "string" },
            "items": {
              "type": "array",
              "items": {
                "type": "object",
                "properties": {
                  "url": { "type": "string" },
                  "text": { "type": "string" },
                  "dataAttributes": {
                    "type": "array",
                    "items": {
                      "type": "object",
                      "properties": {
                        "name": {
                          "type": "string",
                          "enum": [
                            "test",
                            "track",
                            "track-context",
                            "track-value",
                            "track-method",
                            "track-content-type",
                            "track-item-id",
                            "track-type",
                            "track-action",
                            "track-category",
                            "track-label",
                            "track-component"
                          ]
                        },
                        "value": {
                          "type": "string"
                        }
                      },
                      "required": [
                        "name",
                        "value"
                      ]
                    }
                  }
                },
                "required": ["url", "text"]
              }
            }
          },
          "required": ["title", "items"]
        }
      },
      "userDebugInfo": {
        "type": "object",
        "properties": {
          "userDisplayName": {"type": "string"},
          "businessPartners": {
            "type": "array",
            "items": {
              "type": "object",
              "properties": {
                "name": {"type": "string"},
                "id": {"type": "string"}
              }
            }
          },
          "ipAddress": {"type": "string"}
        }
      },
      "image": {
        "type": "object",
        "properties": {
          "src": { "type": "string" },
          "alt": { "type": "string" },
          "link": { "type": "string" }
        },
        "required": ["src", "alt", "link"]
      },
      "currentYear": { "type": "integer" },
      "productFooter": { "type": "string" },
      "corporateFooter": { "type": "string" }
    },
    "required": ["navigation", "image", "currentYear", "productFooter", "corporateFooter"]
  }
  